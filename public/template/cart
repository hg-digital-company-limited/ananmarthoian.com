    <script>
        const couponText = document.getElementById('couponText');
        const couponInputContainer = document.getElementById('couponInputContainer');
        let isCouponInputVisible = false;

        couponText.addEventListener('click', function() {
            isCouponInputVisible = !isCouponInputVisible;

            couponInputContainer.style.display = isCouponInputVisible ? 'flex' : 'none';
        });

        document.getElementById('applyCouponButton').addEventListener('click', function() {
            var couponValue = document.getElementById('couponInput').value;

            var url = "https://vuakhoahoc.org/cart/" + encodeURIComponent(couponValue);

            window.location.href = url;
        });
    </script>

